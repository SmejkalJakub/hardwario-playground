{
  "name": "playground",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "prod": "webpack --progress --config webpack.build.config.js && electron --noDevServer .",
    "build": "webpack --progress --config webpack.build.config.js",
    "package-osx": "webpack --config webpack.build.config.js && electron-packager ./ --overwrite --platform=darwin --arch=x64 --icon=./src/assets/icons/icon.icns --prune=true --out=builds",
    "package-win": "webpack --config webpack.build.config.js && electron-packager ./ --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./src/assets/icons/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"BigClown flasher\"",
    "package-linux": "webpack --config webpack.build.config.js &&  electron-packager ./ --overwrite --platform=linux --arch=x64 --icon=./src/assets/icons/icon.png --prune=true --out=builds"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.23.0",
    "babili-webpack-plugin": "^0.1.2",
    "bootstrap": "^4.1.1",
    "css-loader": "^0.28.11",
    "deep-equal": "^1.0.1",
    "electron": "^1.7.8",
    "electron-json-config": "^1.4.0",
    "electron-packager": "^9.1.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.9.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "express": "^4.16.3",
    "is-port-reachable": "^2.0.0",
    "mosca": "^2.8.1",
    "mqtt": "^2.18.0",
    "node-red": "*",
    "node-red-dashboard": "*",
    "serialport": "^6.2.0"
  }
